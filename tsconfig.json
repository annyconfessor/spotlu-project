{
  "compilerOptions": {
    "jsx": "react-jsx", 
    "moduleResolution": "node",
    "baseUrl": ".",
    "types": ["node", "jest"],
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"]
    },
  },
  "include": [
  "src/**/*",
  ".storybook/**/*"
  ]
}